
datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
  output = "../server/node_modules/.prisma/client"
}

model Thought {
    id          Int     @id @default(autoincrement())
    text        String
}

model User {
    id          Int     @id @default(autoincrement())
    email       String  @unique
    password    String
}

